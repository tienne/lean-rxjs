{
  "gitbook": "3.1.1",
  "title": "Learn RxJS-ko",
  "plugins": [
    "include-codeblock",
    "advanced-emoji",
    "prism",
    "highlight",
    "copy-code-button",
    "edit-link",
    "github",
    "github-buttons",
    "ga"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/tienne/lean-rxjs/tree/master",
      "label": "페이지 수정"
    },
    "github": {
      "url": "https://github.com/tienne/lean-rxjs"
    },
    "ga": {
        "token": ""
    },
    "github-buttons": {
      "repo": "tienne/lean-rxjs",
      "types": [
        "star",
        "watch"
      ],
      "size": "large"
    }
  }
}